{# {% include 'website/pages/supervisor/team.members.twig' %} #}
<div class="row">
	<div class="col-lg-12">
		<p><button type="button" class="btn btn-primary btn-lg btn-block active" aria-expanded="true" data-toggle="collapse" href="#alternate-team-content-container">Team <span class="caret"></span></button></p>
		<h1 class="page-header"></h1>
	</div>
	<div class="col-lg-12 collapse" id="alternate-team-content-container">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12">
					<div class="well well-lg">
						<div class="col-xs-12" id="alternate-team-members-loading" style="display:none">
							<div class="loadme-bounced">
								<div class="loadme-bounced-child loadme-bounced1"></div>
								<div class="loadme-bounced-child loadme-bounced2"></div>
								<div class="loadme-bounced-child loadme-bounced3"></div>
							</div>
							<div class="loadme-mask"></div>
							<br>
						</div>
						<table id="alternate-team-member-table" class="table table-striped table-bordered table-hover" cellspacing="0" width="100%">
							<thead>
								<tr>
									<th>UserID</th>
									<th>Username</th>
					                <th>First Name</th>
					                <th>Middle Name</th>
					                <th>Last Name</th>
					                <th>Phone Number</th>
					                <th>CellPhone</th>
					                <th>Job Title</th>
					                <th>Reporting Unit</th>
					                <th>Email</th>
					                <th>SupervisorID</th>
					                <th>Last Login Date</th>
					                <th>Active</th>
								</tr>
							</thead>
						</table>
						<div class="row" id="alternate-supervisor-team-options">
	                        <div class="col-lg-5" id="alternate-supervisor-team-button-container">
	                            <button type="button" class="btn btn-primary" onclick="SupervisorInterface.present('#team-member-table',{type:'submit',prep: 'prepCreateNewUserSal', isModal:true, select:true, modalName:'#supervisor-create-new-sal-modal', handler:'createUserSal'}, '#supervisor-team-options')">
	                                <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Create New Sal 
	                            </button>
	                            <button type="button" class="btn btn-danger" onclick="UserInterface.present('#team-member-table',{type:'delete', isModal:false, select:true}, '#team-content-container')">
	                                <span class="glyphicon glyphicon-trash" aria-hidden="true"></span> Delete Sal
	                            </button>
	                        </div>
	                        {# {% include 'website/pages/supervisor/modals/supervisor.modal.create.sal.twig' %} #}
	                    </div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="row" id="alternate-supervisor-member-approvals">
	<div class="col-lg-12">
		<h2 class="page-header text-center">Waiting For Approvals</h2>
		{# <button type="button" class="btn btn-primary btn-lg btn-block active">Waiting For Approvals</button> #}
		<br>
		<div class="row">
			<div class="col-lg-12">
				<div class="panel panel-primary" id="alternate-panel-approval-sals" style="display:none">
					<div class="panel-heading text-center">
						<h3 class="panel-title">Name Of Employee</h3>
					</div>
					<div class="panel-body sal-entries" id="alternate-panel-waiting-approval">
					</div>
				</div>
			</div>
		</div>
		<br>
		<div class="row">
			<div class="col-lg-12">
				<div class="row" id="alternate-member-time-compare" style="display:none">
					<div class="col-lg-2 col-md-3 col-sm-3 col-xs-4">
						<div class="panel panel-primary" id="member-sal-time">
							<div class="panel-heading text-center">
								<h3 class="panel-title">Sal Time Entered</h3>
							</div>
							<div class="panel-body text-center">
								<div class="row">
									<div class="col-sm-6" name="minutes">Minutes<p data-minutes="0">0</p></div>
									<div class="col-sm-6" name="hours">Hours<p>0:00</p></div>
								</div>
							</div>
						</div>
					</div>
					<div class="clearfix visible-xs-block"></div>
					<div class="col-lg-2 col-md-3 col-sm-3 col-xs-4">
						<div class="panel panel-primary" id="member-timeips-time">
							<div class="panel-heading text-center">
								<h3 class="panel-title">TimeIPS</h3>
							</div>
							<div class="panel-body text-center">
								<div class="row">
									<div class="col-sm-6" name="minutes" data-minutes="0">Minutes<p>0</p></div>
									<div class="col-sm-6" name="hours">Hours<p>0:00</p></div>
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="well well-lg" id="alternate-member-sal-approval-table-container" style="display:none">
					<table id="alternate-member-sal-approval-table" class="table table-striped table-bordered table-hover" cellspacing="0" width="100%">
						<thead>
							<tr>
								<th>Entry ID</th>
								<th>From Name</th>
								<th>Status</th>
								<th>Reporting Unit</th>
				                <th>Sub Reporting Unit</th>
				                <th>Work Location</th>
				                <th>ActivityCode</th>
				                <th>Project Number</th>
				                <th>Group Number</th>
				                <th>Time Spent</th>
				                <th>Prep Time</th>
				                <th>Description</th>
				                <th>Created</th>
							</tr>
						</thead>
					</table>
					<div class="row" id="alternate-supervisor-sal-approval-options">
		                {# <div class="col-lg-5" id="alternate-supervisor-team-button-container">
		                    <button type="button" class="btn btn-success" onclick="SupervisorInterface.present('#member-sal-approval-table',{type:'submit', isModal:false, select:false, handler:'submitApprovedSalHandler', statusChange:3, head:'#supervisor-member-approvals', closeView:'approvals', closeViews:['#panel-approval-sals','#member-time-compare', '#member-sal-approval-table-container']}, '#supervisor-sal-approval-options')">
		                        <span class="glyphicon glyphicon-ok" aria-hidden="true"></span> Approve Sal 
		                    </button>
		                    <button type="button" class="btn btn-danger" onclick="SupervisorInterface.present('#member-sal-approval-table',{type:'submit',prep: 'prepDenyMemberSal', isModal:true, select:true, modalName:'#member-status-change-modal', handler:'submitDenySalHandler', statusChange:5, closeView:'approvals'}, '#supervisor-sal-approval-options')">
		                        <span class="glyphicon glyphicon-flag" aria-hidden="true"></span> Reject Entry
		                    </button>
		                    <button type="button" class="btn btn-primary" onclick="SupervisorInterface.present('#member-sal-approval-table',{type:'close', isModal:false, select:false, handler:'closeMemberApprovals', closeViews:['#panel-approval-sals','#member-time-compare', '#member-sal-approval-table-container']}, '#supervisor-sal-approval-options')"><span class="glyphicon glyphicon-arrow-up" aria-hidden="true"></span> Close Sal
		                    </button>
		                </div>
		                {% include 'website/pages/supervisor/modals/supervisor.modal.status.change.twig' %} #}
		            </div>
		        </div>
			</div>
		</div>
	</div>
</div>