<div class="row placeholders" id="team-members-container" >
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <p class="navbar-brand">Team Members</p>
        <button type="button" class="btn btn-default navbar-btn" aria-expanded="true" data-toggle="collapse" href="#members-container">Show/Hide</button>
      </div>
    </div>
  </nav>
  <div class="row collapse in" id="members-container">
    {% for member in team %}
        <div class="col-xs-6 col-sm-3 placeholder">
          <a href="javascript:void(0)" data-info='{{member|json_encode|raw}}' onclick="SupervisorInterface.showPendingSalApprovals({member:this, usePanel:true, panelDiv:'#panel-approval-sals', panelEntry:'#panel-waiting-approval', timeCompare:'#member-time-compare', tableName:'#member-sal-approval-table', targetHead:'#supervisor-member-approvals'}, '#team-content-container')"><img src="/images/unknown_user.jpg" width="100" height="100" class="img-responsive" alt="Generic placeholder thumbnail" data-toggle="tooltip" title="Click To See Team Members Open Sal's"></a>
          <h4>{{member['FirstName'] ~ " " ~ member['LastName']}}</h4>
          <span class="text-muted">{{member['Job Title']}}</span>
        </div>
    {% endfor %}
  </div>
</div>