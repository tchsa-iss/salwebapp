<div class="row">
	<div class="col-lg-12">
		<p><button type="button" class="btn btn-primary btn-lg btn-block active">Open</button></p>
		<h1 class="page-header"></h1>
		<div class="row" id="open-sal-container">
			<div class="col-lg-4 col-lg-offset-4 text-center">
				<div class="btn-group" role="group" aria-label="...">
				  <button type="button" class="btn btn-default" onclick="UserInterface.getSalsWithOption(0, {tableName:'#sal-open-table',usePanel:false, panelDiv:'#open-sals-panel', salStatus:1})">Today</button>
				  <button type="button" class="btn btn-default" onclick="UserInterface.getSalsWithOption(1, {tableName:'#sal-open-table', usePanel:true, panelDiv:'#open-sals-panel', panelEntry:'#panel-open-sal-entries', salStatus:1})">Week</button>
				  <button type="button" class="btn btn-default" onclick="UserInterface.getSalsWithOption(2, {tableName:'#sal-open-table', usePanel:true, panelDiv:'#open-sals-panel', panelEntry:'#panel-open-sal-entries', salStatus:1})">Month</button>
				  <button type="button" class="btn btn-default" onclick="UserInterface.getSalsWithOption(3,{rangeDiv:'#open-sal-date-range', elements:['#sal-open-from-date','#sal-open-to-date'], tableName:'#sal-open-table', usePanel:true, panelDiv:'#open-sals-panel', panelEntry:'#panel-open-sal-entries', salStatus:1})">Date Range</button>
				</div>
			</div>
		</div>
		<br>
		<div class="row">
			<div class="col-lg-12 col-xs-12 text-center" id="open-sal-date-range" style="display:none">
				<form class="form-inline">
				  <div class="form-group">
				    <label for="exampleInputName1">From</label>
				    <input type="text" class="form-control" id="sal-open-from-date" placeholder="">
				  </div>
				  <div class="form-group">
				    <label for="exampleInputName2">to</label>
				    <input type="text" class="form-control" id="sal-open-to-date" placeholder="">
				  </div>
				  <div class="form-group">
				    <div class="col-sm-10">
				      <button type="submit" class="btn btn-primary">Submit</button>
				    </div>
				  </div>
				</form>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12" id="open-sals-loading" style="display:none">
				<div class="loadme-bounced">
					<div class="loadme-bounced-child loadme-bounced1"></div>
					<div class="loadme-bounced-child loadme-bounced2"></div>
					<div class="loadme-bounced-child loadme-bounced3"></div>
				</div>
				<div class="loadme-mask"></div>
				<br>
			</div>
		</div>
		<br>
		<div class="panel panel-primary" id="open-sals-panel" style="display:none">
			<div class="panel-heading text-center">
				<h3 class="panel-title">Sal's</h3>
			</div>
			<div class="panel-body sal-entries" id="panel-open-sal-entries">
			</div>
		</div>
		<div class="row" id="sal-open-table-container" style="display:none">
			<div class="col-lg-12">
				<div class="row sal-time-compare">
					<div class="col-lg-2 col-md-3 col-sm-3 col-xs-4">
						<div class="panel panel-primary member-time">
							<div class="panel-heading text-center">
								<h3 class="panel-title">Time Entered</h3>
							</div>
							<div class="panel-body text-center">
								<div class="row">
									<div class="col-sm-6">Minutes<p name="minutes" data-minutes='{"time":0}'>0</p></div>
									<div class="col-sm-6">Hours<p name="hours">0:00</p></div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-lg-2 col-md-3 col-sm-3 col-xs-4">
						<div class="panel panel-primary timeips-time">
							<div class="panel-heading text-center">
								<h3 class="panel-title">TimeIPS</h3>
							</div>
							<div class="panel-body text-center">
								<div class="row">
									<div class="col-sm-6">Minutes<p name="minutes">0</p></div>
									<div class="col-sm-6">Hours<p name="hours">0:00</p></div>
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="well well-lg">
					<table id="sal-open-table" class="table table-striped table-bordered table-hover" cellspacing="0" width="100%">
						<thead>
							<tr>
								<th>Entry ID</th>
								<th>From Name</th>
								<th>Status</th>
								<th>Reporting Unit</th>
				                <th>Sub Reporting Unit</th>
				                <th>Work Location</th>
				                <th>ActivityCode</th>
				                <th>Project Number</th>
				                <th>Group Number</th>
				                <th>Time Spent</th>
				                <th>Prep Time</th>
				                <th>Description</th>
				                <th>Created</th>
							</tr>
						</thead>
					</table>
					<div class="row" id="open-sal-options">
                        <div class="col-lg-12" id="user-roles-button-container">
                            <button type="button" class="btn btn-primary" onclick="UserInterface.presentModal('#sal-open-table',{type:'add',prep: 'prepSalAddEntry', isModal:true, select:false, modalName:'#sal-entry-modal',handler:'salAddEntrySubmitHandler'}, '#open-sal-container')">
                                <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Add Entry 
                            </button>
                            <button type="button" class="btn btn-warning" onclick="UserInterface.presentModal('#sal-open-table',{type:'modify', prep: 'prepModifySalEntry', isModal:true, select:true, modalName:'#sal-entry-modal',handler:'salEditEntrySubmitHandler'}, '#open-sal-options')">
                                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Edit Entry 
                            </button>
                            <button type="button" class="btn btn-danger" onclick="UserInterface.presentModal('#sal-open-table',{type:'delete', prep: 'prepDeleteSalEntry', isModal:true, modalName:'#delete-sal-entry-modal', select:true, handler:'salDeleteHandler'}, '#open-sal-options')">
                                <span class="glyphicon glyphicon-trash" aria-hidden="true"></span> Delete Entry
                            </button>
                            <button type="button" class="btn btn-success" onclick="UserInterface.presentModal('#sal-open-table',{type:'submit',prep: 'prepSubmitSal', isModal:true, select:false, modalName:'#submit-sal-modal', handler:'salSubmitHandler',statusChange:2, closeView:'open'}, '#open-sal-container')">
                                <span class="glyphicon glyphicon-ok" aria-hidden="true"></span> Submit 
                            </button>
                        </div>
                        {# {% include 'website/pages/sal/modals/modal.sal.entry.twig' %} #}
                        {# {% include 'website/pages/sal/modals/modal.delete.sal.twig' %} #}
                        {% include 'website/pages/sal/modals/modal.submit.sal.twig' %}
                    </div>
				</div>
			</div>
		</div>
	</div>
</div>