<div class="row">
	<div class="col-lg-12">
		<p><button type="button" class="btn btn-primary btn-lg btn-block active">Corrections</button></p>
		<h1 class="page-header"></h1>
		<div class="row">
			<div class="col-xs-12" id="sal-corrections-loading">
				<div class="loadme-bounced">
					<div class="loadme-bounced-child loadme-bounced1"></div>
					<div class="loadme-bounced-child loadme-bounced2"></div>
					<div class="loadme-bounced-child loadme-bounced3"></div>
				</div>
				<div class="loadme-mask"></div>
				<br>
			</div>
		</div>
		<div class="panel panel-primary" id="correction-sals-panel">
			<div class="panel-heading text-center">
				<h3 class="panel-title">SAL Errors</h3>
			</div>
			  <div class="panel-body sal-entries" id="panel-correction-sal-entries">
			  </div>
		</div>
		<div class="well well-lg" id="member-sal-correction-table-container" style="display:none">
			<table id="member-sal-correction-table" class="table table-striped table-bordered table-hover" cellspacing="0" width="100%">
				<thead>
					<tr>
						<th>Entry ID</th>
						<th>From Name</th>
						<th>Status</th>
						<th>Reporting Unit</th>
		                <th>Sub Reporting Unit</th>
		                <th>Work Location</th>
		                <th>ActivityCode</th>
		                <th>Project Number</th>
		                <th>Group Number</th>
		                <th>Time Spent</th>
		                <th>Prep Time</th>
		                <th>Description</th>
		                <th>Created</th>
					</tr>
				</thead>
			</table>
			<div class="row" id="sal-resubmit-progress" style="display:none">
				<div class="col-lg-12 text-center">
					<div class="panel panel-default">
						<div class="panel-body">
							<h4 class="text-center" style="color:#777;">Updating....</h4>
					  		<div class="progress" style="margin-bottom:0px;">
							  	<div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;">
							    	<span class="sr-only">0% Complete</span>
							  	</div>
							</div>
					  	</div>
					</div>
				</div>
			</div>
			<br>
			<div class="row" id="correction-sal-error-message">
				<div class="panel panel-default">
					<div class="panel-heading text-center" style="background:#d9534f; color:white; font-size:16">Entry Errors</div>
						<div class="panel-body">
					    	<ul class="list-group">
							  <li class="list-group-item">Entry ID</li>
							</ul>
				  	</div>
				</div>
			</div>
			<div class="row" id="correction-sal-options">
                <div class="col-lg-12">
                    <button type="button" class="btn btn-warning" onclick="UserInterface.presentModal('#member-sal-correction-table',{type:'modify',prep: 'prepModifySalEntry', isModal:true, select:true, modalName:'#sal-entry-modal',handler:'salEditEntrySubmitHandler', closeView:'approved'}, '#correction-sal-options')">
                        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Edit Entry 
                    </button>
                    <button type="button" class="btn btn-danger" onclick="UserInterface.presentModal('#member-sal-correction-table',{type:'delete', prep: 'prepDeleteSalEntry', isModal:true, modalName:'#delete-sal-entry-modal', select:true, handler:'salDeleteHandler'}, '#open-sal-options')">
                        <span class="glyphicon glyphicon-trash" aria-hidden="true"></span> Delete Entry
                    </button>
                    <button type="button" class="btn btn-success" onclick="UserInterface.presentModal('#member-sal-correction-table',{type:'submit', isModal:false, select:false, handler:'updateUserSalStatus', closeView:'corrections', statusChange:2, progressView:'#sal-close-progress'}, '#correction-sal-options')">
                    	<span class="glyphicon glyphicon-repeat" aria-hidden="true"></span> Re-Submit SAL 
                    </button>
                </div>
            </div>
        </div>
	</div>
</div>