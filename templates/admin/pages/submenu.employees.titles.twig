<div class="row employees-menu" style="display:none">
    <button type="button" onclick="AdminInterface.onEmployeeJobTitlesClick('#job-titles-table-container')" class="btn btn-default btn-lg btn-block active">Employess Job Titles</button>
</div>
<div class="well well-lg" id="job-titles-table-container" style="display:none">
	<div class="row">
		<div class="col-lg-12">
	        <table id="job-titles-table" class="table table table-striped table-bordered table-hover" cellspacing="0" width="100%">
	            <thead>
	                <tr>
	                    <th>Id</th>
	                    <th>Name</th>
	                    <th>Description</th>
	                </tr>
	            </thead>
	        </table>
	    </div>
	</div>
	<div class="row" id="account-codes-options">
        <div class="col-lg-5" id="user-roles-button-container">
            <button type="button" class="btn btn-primary" onclick="AdminInterface.onClick('#account-codes-table',{type:'add'}, '#activity-code-container')">
                <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Job Title
            </button>
            <button type="button" class="btn btn-default" onclick="AdminInterface.onClick('#account-codes-table',{type:'modify'}, '#activity-code-container')">
                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Modify
            </button>
            <button type="button" class="btn btn-danger" onclick="AdminInterface.onClick('#account-codes-table',{type:'delete'}, '#activity-code-container')">
                <span class="glyphicon glyphicon-trash" aria-hidden="true"></span> Delete
            </button>
        </div>
    </div>
</div>